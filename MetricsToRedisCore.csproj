<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Metrics to redis (Core)</Title>
    <Version>0.0.3</Version>
    <Authors>Konstatin Chizhov</Authors>
    <PackageProjectUrl>https://github.com/kxk911/MetricsToRedisCore</PackageProjectUrl>
    <RepositoryUrl>https://github.com/kxk911/MetricsToRedisCore</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="StackExchange.Redis.Extensions.Core" Version="10.2.0" />
  </ItemGroup>

</Project>
